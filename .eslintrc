{
    "extends": [
        "react-app"
    ],
    "plugins": [
        "prettier",
        "react",
        "react-hooks",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "tabWidth": 4,
                "trailingComma": "all",
                "printWidth": 100
            }
        ],
        "eqeqeq": [
            "error",
            "always"
        ],
        "no-console": "warn"
    }
}
